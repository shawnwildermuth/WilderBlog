@{
  ViewBag.Title = "Shawn Wildermuth's Rants and Raves";
  ViewBag.PageName = "Contact Me";
  ViewBag.PageBlurb = "Send a Message to Shawn Wildermuth";
  ViewBag.PageImage = "/img/headers/about.jpg";
}
@section Scripts {
  <environment names="Development">
    <!-- IE required polyfills, in this exact order -->
    <script src="~/lib/angular2/es6-shim.js"></script>
    <script src="~/lib/angular2/system-polyfills.src.js"></script>

    <script src="~/lib/angular2/system.src.js"></script>
    <script src="~/lib/angular2/Rx.js"></script>

    <script src="~/app/systemjs.config.js"></script>
  </environment>
  <environment names="Production">
    <!-- IE required polyfills, in this exact order -->
    <script src="~/lib/angular2/es6-shim.min.js"></script>
    <script src="~/lib/angular2/system-polyfills.src.js"></script>

    <script src="~/lib/angular2/system.js"></script>
    <script src="~/lib/angular2/Rx.min.js"></script>

    <script src="~/app/systemjs.config.js"></script>
  </environment>
  <script>
    System.import('./app/main')
          .then(null, console.error.bind(console));
  </script>
}
<div class="row">
  <div class="offset-lg-3 col-lg-6">
    <contact-form><div style="text-align: center;"><i class="fa fa-rebel fa-spin fa-lg"></i></div></contact-form>
  </div>
</div>
<hr/>